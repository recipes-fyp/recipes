<form id='delete' method="post" action="{{ path('coll_delete', {'id': coll.id}) }}" onsubmit="return confirm('Are you sure you want to delete this Collection?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ coll.id) }}">
    <button class="btn">Delete</button>
</form>